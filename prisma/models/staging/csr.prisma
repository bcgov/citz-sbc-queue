model CSR {
  csrId                 Int       @id @default(autoincrement()) @map("csr_id")
  username              String    @unique @db.VarChar(150)

  /// FKs
  officeId              Int       @map("office_id")
  counterId             Int?      @map("counter_id")
  roleId                Int       @map("role_id")
  csrStateId            Int       @map("csr_state_id")

  /// Historical / soft-delete
  deleted               DateTime? @db.Timestamp(6)

  /// Flags
  qtXnCsrInd            Int       @map("qt_xn_csr_ind") // Deprecated
  receptionistInd       Int       @map("receptionist_ind")
  officeManager         Int       @map("office_manager")
  pesticideDesignate    Int       @map("pesticide_designate")
  financeDesignate      Int       @map("finance_designate")
  ita2Designate         Int       @map("ita2_designate")

  @@map("csr")
  @@schema("public")
}
