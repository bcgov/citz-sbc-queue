model Exam {
  examId                      Int       @id @default(autoincrement()) @map("exam_id")
  bookingId                   Int?      @map("booking_id")
  examTypeId                  Int?      @map("exam_type_id")
  officeId                    Int       @map("office_id")
  eventId                     String?   @db.VarChar(25) @map("event_id")
  examName                    String    @db.VarChar(50) @map("exam_name")
  examineeName                String?   @db.VarChar(50) @map("examinee_name")
  expiryDate                  DateTime? @db.Timestamp(6) @map("expiry_date")
  notes                       String?   @db.VarChar(400)
  sessionNumber               Int?      @map("session_number")
  numberOfStudents            Int?      @map("number_of_students")
  examMethod                  String    @db.VarChar(15) @map("exam_method")
  deletedDate                 String?   @map("deleted_date")
  examReceivedDate            DateTime? @db.Timestamp(6) @map("exam_received_date")
  examReturnedTrackingNumber  String?   @db.VarChar(255) @map("exam_returned_tracking_number")
  offsiteLocation             String?   @db.VarChar(255) @map("offsite_location")
  examReturnedDate            DateTime? @db.Timestamp(6) @map("exam_returned_date")
  examWrittenInd              Int       @map("exam_written_ind")
  bcmpJobId                   String?   @db.VarChar(100) @map("bcmp_job_id")
  examDestroyedDate           String?   @map("exam_destroyed_date")
  uploadReceivedInd           Int?      @map("upload_received_ind")
  payeeEmail                  String?   @db.VarChar(50) @map("payee_email")
  payeeInd                    Int?      @map("payee_ind")
  payeeName                   String?   @db.VarChar(50) @map("payee_name")
  payeePhone                  String?   @db.VarChar(50) @map("payee_phone")
  receipt                     String?   @db.VarChar(50)
  receiptSentInd              Int?      @map("receipt_sent_ind")
  sbcManagedInd               Int?      @map("sbc_managed_ind")
  examineeEmail               String?   @db.VarChar(400) @map("examinee_email")
  examineePhone               String?   @db.VarChar(400) @map("examinee_phone")
  invigilatorId               Int?      @map("invigilator_id")
  candidatesList              Json?     @map("candidates_list")
  isPesticide                 Int?      @map("is_pesticide")

  @@map("exam")
  @@schema("public")
}
