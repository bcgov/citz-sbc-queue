model AppointmentVersionArchive {
  appointmentId    Int       @map("appointment_id")
  officeId         Int?      @map("office_id")
  serviceId        Int?      @map("service_id")
  citizenId        Int?      @map("citizen_id")
  startTime        DateTime? @db.Timestamp(6) @map("start_time")
  endTime          DateTime? @db.Timestamp(6) @map("end_time")
  checkedInTime    DateTime? @db.Timestamp(6) @map("checked_in_time")
  comments         String?   @db.VarChar(255)
  citizenName      String?   @db.VarChar(255) @map("citizen_name")
  contactInformation String? @db.VarChar(255) @map("contact_information")
  blackoutFlag     String?   @db.VarChar(1) @map("blackout_flag")
  recurringUuid    String?   @db.VarChar(255) @map("recurring_uuid")
  onlineFlag       Boolean?  @map("online_flag")
  isDraft          Boolean?  @map("is_draft")
  createdAt        DateTime? @db.Timestamp(6) @map("created_at")
  statFlag         Boolean?  @map("stat_flag")
  updatedAt        DateTime? @db.Timestamp(6) @map("updated_at")
  updatedBy        String?   @map("updated_by")
  transactionId    Int       @map("transaction_id")
  endTransactionId Int?      @map("end_transaction_id")
  operationType    Int       @map("operation_type")

  appointment AppointmentLegacy @relation(fields: [appointmentId], references: [appointmentId])
  office      Office? @relation(fields: [officeId], references: [officeId])
  transaction Transaction @relation(fields: [transactionId], references: [id])

  @@id([appointmentId, transactionId])
  @@map("appointment_version_archive")
  @@schema("public")
}
