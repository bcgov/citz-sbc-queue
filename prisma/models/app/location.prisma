model Location {
  id                 String   @id @default(uuid())
  name               String   @db.VarChar(250)
  timezone           String   @db.VarChar(50)
  streetAddress      String   @db.VarChar(500) @map("street_address")
  mailAddress        String?  @db.VarChar(500) @map("mail_address")
  phoneNumber        String?  @db.VarChar(20) @map("phone_number")
  latitude           Float
  longitude          Float
  legacyOfficeNumber Int?     @unique @map("legacy_office_number")

  staffUsers          StaffUser[]

  deletedAt          DateTime? @db.Timestamp(6) @map("deleted_at")
  createdAt          DateTime @default(now()) @db.Timestamp(6) @map("created_at")
  updatedAt          DateTime @updatedAt @db.Timestamp(6) @map("updated_at")

  @@map("location")
  @@schema("app")
}
